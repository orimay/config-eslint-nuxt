{
  "name": "orimay-config-eslint-nuxt",
  "version": "1.0.4",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./src": "./index.mts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "nuxt",
    "typescript",
    "lint",
    "flat-config"
  ],
  "scripts": {
    "release": "changelogen --release --push && bun publish",
    "lint": "eslint . --fix",
    "prepare": "husky",
    "build": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^24.1.0",
    "camelcase": "^8.0.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.0",
    "lint-staged": "^16.1.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "dependencies": {
    "eslint-flat-config-utils": "^2.1.0",
    "orimay-config-eslint": "workspace:*"
  },
  "peerDependencies": {
    "eslint": "^9.31.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "lint-staged": {
    "*.mts": "bun lint"
  }
}
