{
  "name": "orimay-config-eslint-nuxt",
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/eslint.config.mjs",
  "types": "./dist/eslint.config.d.mts",
  "exports": {
    ".": {
      "import": "./dist/eslint.config.mjs",
      "require": "./dist/eslint.config.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "nuxt",
    "typescript",
    "lint",
    "flat-config"
  ],
  "scripts": {
    "release": "changelogen --release --push && pnpm publish",
    "lint": "eslint . --fix",
    "prepare": "husky",
    "build": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.10.1",
    "camelcase": "^8.0.0",
    "changelogen": "^0.5.7",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.2.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "eslint-flat-config-utils": "^2.0.1",
    "orimay-config-eslint": "latest"
  },
  "peerDependencies": {
    "eslint": "^9.16.0"
  },
  "lint-staged": {
    "*.mts": "pnpm run lint"
  }
}
